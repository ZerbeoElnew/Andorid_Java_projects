<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F8FBFE"
    android:backgroundTint="#00851D1D"
    android:backgroundTintMode="add"
    android:textAlignment="center"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="255dp"
        android:layout_height="219dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="81dp"
        android:layout_marginEnd="75dp"
        android:layout_marginBottom="498dp"
        app:srcCompat="@drawable/facebook_logo_2023" />

    <EditText
        android:id="@+id/editTextText8"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="22dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="439dp"
        android:ems="20"
        android:inputType="text"
        android:text="Name" />

    <EditText
        android:id="@+id/editTextText9"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/editTextText8"
        android:layout_alignTop="@+id/editTextText8"
        android:layout_alignEnd="@+id/editTextText8"
        android:layout_alignBottom="@+id/editTextText8"
        android:layout_marginStart="1dp"
        android:layout_marginTop="69dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="-69dp"
        android:ems="20"
        android:inputType="text"
        android:text="Name" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/editTextText9"
        android:layout_alignTop="@+id/editTextText9"
        android:layout_alignEnd="@+id/editTextText9"
        android:layout_alignBottom="@+id/editTextText9"
        android:layout_marginStart="1dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="-60dp"
        android:background="#0339F8"
        android:text="Button"
        android:textColor="#FEFEFE" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button3"
        android:layout_width="371dp"
        android:layout_height="63dp"
        android:layout_alignStart="@+id/button2"
        android:layout_alignTop="@+id/button2"
        android:layout_alignEnd="@+id/button2"
        android:layout_alignBottom="@+id/button2"
        android:layout_marginStart="0dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="-3dp"
        android:layout_marginBottom="-78dp"
        android:backgroundTint="#F8FBFE"
        android:text="Forgot Password ?"
        android:textColor="#0866FE"
        android:textColorLink="#00FFFFFF" />

    <View
        android:id="@+id/view"
        android:layout_width="194dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/button3"
        android:layout_alignTop="@+id/button3"
        android:layout_alignEnd="@+id/button3"
        android:layout_alignBottom="@+id/button3"
        android:layout_marginStart="5dp"
        android:layout_marginTop="84dp"
        android:layout_marginEnd="220dp"
        android:layout_marginBottom="-26dp"
        android:background="#505050" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="46dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/button3"
        android:layout_alignEnd="@+id/button3"
        android:layout_alignBottom="@+id/button3"
        android:layout_marginTop="74dp"
        android:layout_marginEnd="164dp"
        android:layout_marginBottom="-29dp"
        android:text="OR"
        android:textAlignment="center" />

    <View
        android:id="@+id/view4"
        android:layout_width="wrap_content"
        android:layout_height="4dp"
        android:layout_alignStart="@+id/view"
        android:layout_alignTop="@+id/view"
        android:layout_alignEnd="@+id/view"
        android:layout_alignBottom="@+id/view"
        android:layout_marginStart="220dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="-210dp"
        android:layout_marginBottom="0dp"
        android:background="#505050" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button6"
        android:layout_width="369dp"
        android:layout_height="58dp"
        android:layout_alignStart="@+id/button2"
        android:layout_alignTop="@+id/button2"
        android:layout_alignEnd="@+id/button2"
        android:layout_alignBottom="@+id/button2"
        android:layout_marginStart="0dp"
        android:layout_marginTop="195dp"
        android:layout_marginEnd="-2dp"
        android:layout_marginBottom="-204dp"
        android:background="#2D302B"
        android:backgroundTint="#07A515"
        android:text="Button"
        android:textColor="#FDFDFD" />

</RelativeLayout>